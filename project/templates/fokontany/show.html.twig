{% extends 'base.html.twig' %}

{% block title %}Fokontany{% endblock %}

{% block body %}
    <h1>Fokontany</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fokontany.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ fokontany.nom }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ fokontany.createdAt ? fokontany.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ fokontany.updatedAt ? fokontany.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_fokontany_index') }}">back to list</a>

    <a href="{{ path('app_fokontany_edit', {'id': fokontany.id}) }}">edit</a>

    {{ include('fokontany/_delete_form.html.twig') }}
{% endblock %}
